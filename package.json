{
  "name": "@rgossiaux/svelte-headlessui",
  "description": "A complete, full-featured, unofficial Svelte port of Headless UI, an unstyled, fully accessible UI component library.",
  "keywords": [
    "svelte",
    "headlessui",
    "svelte3",
    "svelte-components",
    "headless",
    "sveltejs"
  ],
  "homepage": "https://svelte-headlessui.goss.io",
  "bugs": {
    "url": "https://github.com/rgossiaux/svelte-headlessui/issues"
  },
  "license": "MIT",
  "repository": "github:rgossiaux/svelte-headlessui",
  "version": "1.0.2",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "vite package && find package/ -type f -exec sed -i '' -e 's/process.env.NODE_ENV/\"production\"/' {} \\;",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "jest"
  },
  "peerDependencies": {
    "svelte": "^3.44.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.4.3",
    "@rgossiaux/svelte-heroicons": "^0.1.2",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.7.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.4.3",
    "cssnano": "^5.1.15",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-inline-compile": "^0.2.0",
    "svelte-jester": "^2.3.2",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^5.0.1",
    "svelte2tsx": "^0.6.1",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.2"
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@unocss/reset": "^0.49.7",
    "@unocss/vite": "^0.49.7",
    "unocss": "^0.49.7"
  }
}
